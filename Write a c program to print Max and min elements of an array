#include<stdio.h>
int main()
{
    int arr[]={2,3,4,5},i;
    int max=arr[0];
    int min=arr[0];
    int length=sizeof(arr)/sizeof(arr[0]);
    for(i=1;i<length;i++)
    {
        if(arr[i]>max)
        max=arr[i];
        if(arr[i]<min)
        min=arr[i];
    }
   printf("min and max elements are:%d\n%d",min,max); 
}
